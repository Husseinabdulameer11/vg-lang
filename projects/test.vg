import OSLib.sys;

function main() {
    var osName = sys.getOSName();
    print("Operating System: " + osName);

    var userHome = sys.getUserHome();
    print("User Home Directory: " + userHome);

    var userDir = sys.getUserDir();
    print("Current Working Directory: " + userDir);

    var pathEnv = sys.getEnv("PATH");
    print("PATH environment variable: " + pathEnv);
}

main();
